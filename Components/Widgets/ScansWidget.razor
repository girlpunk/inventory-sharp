@using InventorySharp.Models
@using InventorySharp.Services
@inherits ComputedStateComponent<ICollection<LabelScan>>
@inject IScanService ScanService

<h3>ScansWidget</h3>

@foreach (var scan in State.Value)
{
    <div>
        Label ID: @scan.LabelId <br />
        Latitude: @scan.Latitude <br />
        Longitude: @scan.Longitude <br />
        Scanner: @scan.Scanned <br />
        Scanner: @scan.ScannerId <br />
        Scan Type: @scan.ScanType < br />
    </div>
}
